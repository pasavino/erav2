{
  "expo": {
    "name": "EraV2",
    "slug": "EraV2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "platforms": [
      "android"
    ],
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/icon.png",
      "newArchEnabled": true,
      "bundleIdentifier": "com.easyrideafrica.erav2"
    },
    "android": {
      "package": "com.easyrideafrica.erav2",
      "jsEngine": "hermes",
      "versionCode": 5,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-1024-transparent.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "newArchEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a2e91259-61fb-40e4-8b4b-89ca53e5d0fc"
      }
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
